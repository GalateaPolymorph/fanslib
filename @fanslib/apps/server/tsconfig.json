{
  "extends": "@fanslib/typescript/bun.json",
  "compilerOptions": {
    "paths": {
      "~/*": ["./src/*"]
    },
    "noEmit": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
