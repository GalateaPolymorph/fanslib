{
  "name": "fanslib",
  "productName": "FansLib",
  "version": "1.0.0",
  "description": "A tool for managing adult content creators' content libraries.",
  "main": "./out/main/index.js",
  "author": "Galatea Polymorph",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:check": "eslint src --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "check": "npm run format:check && npm run lint:check && npm run typecheck",
    "unused": "ts-unused-exports tsconfig.web.json --showLineNumber",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "schema:fetch": "ts-node scripts/fetch-schema.ts",
    "codegen": "graphql-codegen",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
